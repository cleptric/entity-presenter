{
    "name": "cleptric/entity-presenter",
    "description": "Entity Presenter plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": ">=3.3.2 <4.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "<6.0",
        "cakephp/cakephp-codesniffer": "^2.1",
        "phpstan/phpstan": "^0.6"
    },
    "autoload": {
        "psr-4": {
            "EntityPresenter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EntityPresenter\\Test\\": "tests",
            "TestApp\\": "tests/test_app/TestApp"
        }
    }
}
